C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;MAE9C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACP,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACtC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACxB,CAAC;;IAED,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACtB,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;IAE3B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE;MACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACX,CAAC,CAAC;;IAEF,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;MACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACP,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACN,CAAC,CAAC,CAAC,CAAC;MACJ,CAAC,CAAC,CAAC;MACH,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACZ,CAAC;EACH,CAAC;AACH,CAAC","file":"C:/F/vueProject/learnvue/src/components/Cheap.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <div class=\"weui-panel weui-panel_access\">\n      <div\n        v-for=\"(item, index) in data\"\n        :key=\"index\"\n        class=\"weui-panel__bd\"\n        @click=\"\n          getTkl(item.itemid);\n          dialogView = true;\n        \"\n        style=\"cursor: pointer\"\n      >\n        <a class=\"weui-media-box weui-media-box_appmsg\" target=\"_blank\">\n          <div class=\"weui-media-box__hd\">\n            <img\n              class=\"weui-media-box__thumb\"\n              :src=\"item.itempic + '_100x100.jpg'\"\n            />\n          </div>\n          <div class=\"weui-media-box__bd\">\n            <h4 class=\"weui-media-box__title\" v-text=\"item.itemtitle\"></h4>\n            <h4 class=\"weui-media-box__title\">\n              券后包邮仅需: {{ item.itemendprice }} || 机器人再返:\n              {{ (item.tkmoney * 0.6).toFixed(2) }}\n            </h4>\n            <p class=\"weui-media-box__desc\" v-text=\"item.itemdesc\"></p>\n          </div>\n        </a>\n      </div>\n      <div @click=\"loadHot\" class=\"weui-panel__ft\">\n        <a\n          href=\"javascript:void(0);\"\n          class=\"weui-cell weui-cell_access weui-cell_link\"\n        >\n          <div class=\"weui-cell__bd\">查看更多</div>\n          <span class=\"weui-cell__ft\"></span>\n        </a>\n      </div>\n    </div>\n    <div class=\"weui-dialog\" v-if=\"dialogView\">\n      <div class=\"weui-dialog__hd\">\n        <strong class=\"weui-dialog__title\" id=\"js_title1\">温馨提示</strong>\n      </div>\n      <div class=\"weui-dialog__bd\">\n        如果打开手淘客户端没有显示的话,请手动复制以下淘口令再打开\n        <br />\n        {{ tkl }}\n      </div>\n      <div class=\"weui-dialog__ft\">\n        <a\n          role=\"button\"\n          class=\"weui-dialog__btn weui-dialog__btn_primary\"\n          @click=\"\n            dialogView = false;\n            tkl = '';\n          \"\n          >我知道了</a\n        >\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nimport { onMounted, reactive, ref, toRef, toRefs } from \"@vue/runtime-core\";\nimport useClipboard from \"vue-clipboard3\";\nexport default {\n  setup() {\n    //获取优惠信息\n    let data = reactive([]);\n    let page = 1;\n\n    let loadHot = async () => {\n      let getData = await axios.post(\"api/spk/jiukuaijiu\", {\n        apikey: \"ycq9hxGGYdUG7GTdkHBhQQ77UsiZFW2h\",\n        nav: 1,\n        page_size: 20,\n        min_id: page,\n        sort: 0,\n      });\n\n      //每次点击重新加载 就添加页数\n      page++;\n      //添加从淘宝联盟获取的商品数据\n      data.push(...getData.data.data.data);\n    };\n\n    //淘口令\n    let tkl = ref(\"\");\n    //点击商品信息的时候 获取淘口令\n    let getTkl = async (itemid) => {\n      let tklData = await axios.post(\"api/tbk/id_privilege\", {\n        apikey: \"ycq9hxGGYdUG7GTdkHBhQQ77UsiZFW2h\",\n        id: itemid,\n        tpwd: true,\n      });\n\n      //淘口令\n      tkl.value = tklData.data.data.long_coupon_tpwd;\n\n      //复制到粘贴板\n      const { toClipboard } = useClipboard();\n      toClipboard(tkl.value);\n    };\n\n    //控制点击商品信息后的提示框 是否打开/关闭\n    let dialogView = ref(false);\n\n    onMounted(() => {\n      loadHot();\n    });\n\n    return {\n      loadHot,\n      getTkl,\n      data,\n      tkl,\n      dialogView,\n    };\n  },\n};\n</script>\n\n<style>\n.imgBox {\n  margin: 10px;\n}\n.weui-dialog__hd {\n  color: RGB(0, 128, 0);\n}\n</style>"]}]}